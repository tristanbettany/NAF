{
    "name": "tristanbettany/ptcg-collection-manager",
    "type": "project",
    "description": "PTCG Collection Manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Tristan Bettany",
            "homepage": "http://www.tristanbettany.com",
            "email": "dev@tristanbettany.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.3",
        "league/container": "^4.1",
        "league/route": "^5.1",
        "guzzlehttp/guzzle": "^7.3",
        "laminas/laminas-diactoros": "^2.6",
        "laminas/laminas-httphandlerrunner": "^2.0",
        "league/config": "^1.1",
        "filp/whoops": "^2.14",
        "twig/twig": "^3.3",
        "doctrine/migrations": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Presentation\\": "Presentation/",
            "Application\\": "Application/",
            "Domain\\": "Domain/",
            "Database\\": "Database/",
            "Infrastructure\\": "Infrastructure/"
        },
        "files": [
            "Infrastructure/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
